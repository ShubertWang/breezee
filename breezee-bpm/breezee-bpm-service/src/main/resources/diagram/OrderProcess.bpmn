      <sequenceFlow id="flow13" sourceRef="orderDelivery" targetRef="endevent1"></sequenceFlow>
      <sequenceFlow id="flow14" sourceRef="orderPickup" targetRef="endevent1"></sequenceFlow>
      <sequenceFlow id="flow15" name="Reject" sourceRef="orderConfirm" targetRef="endevent1"></sequenceFlow>
    </subProcess>
    <serviceTask id="complete" name="Complete" activiti:delegateExpression="bbb"></serviceTask>
    <endEvent id="endevent2" name="End"></endEvent>
    <sequenceFlow id="flow1" sourceRef="start" targetRef="fulfillment"></sequenceFlow>
    <sequenceFlow id="flow2" sourceRef="fulfillment" targetRef="needPay"></sequenceFlow>
    <sequenceFlow id="noneed" name="No" sourceRef="needPay" targetRef="orderHandle"></sequenceFlow>
    <sequenceFlow id="yesneed" name="Yes" sourceRef="needPay" targetRef="payment">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${needPay=='Y'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow5" sourceRef="payment" targetRef="orderHandle"></sequenceFlow>
    <sequenceFlow id="flow6" sourceRef="orderHandle" targetRef="complete"></sequenceFlow>
    <sequenceFlow id="flow7" sourceRef="complete" targetRef="endevent2"></sequenceFlow>
